 <%= form_for @user do |f| %>
 	<br><br>
 	<%= f.label :username %>
 	<%= f.text_field :username %>
 	<br><br>
 
 	<%= f.label :fname %>
 	<%= f.text_field :fname %>
 	<%= f.label :lname %>
 	<%= f.text_field :lname %>
 	<br><br>
 	<%= f.label :email %>
 	<%= f.text_field :email %>
 	<br><br>
 	<%= f.label :password %>
 	<%= f.password_field :password %>
 	<%= f.submit %>



 <% end %>